          }
        },
        "parameters": [
          {
            "name": "recipient",
            "description": "recipient is the recipient address to query unclaimed budget amount for.",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "tags": [
          "Query"
        ]
      }
    },
    "/cosmos/protocolpool/v1/params": {
      "get": {
